<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#79BAEC">

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline29"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="89dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline30"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="330dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline31"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="572dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:fontFamily="@font/maplestorybold"
        android:text="재미있는 계산기 놀이"
        android:textSize="30dp"
        app:layout_constraintBottom_toTopOf="@+id/guideline29"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.591"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/num2"
        android:layout_width="50dp"
        android:layout_height="55dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/rectangle"
        android:fontFamily="@font/maplestorybold"
        android:gravity="center"
        android:text="9"
        android:textColor="#000"
        android:textSize="30dp"
        app:layout_constraintStart_toEndOf="@+id/sign"
        app:layout_constraintTop_toTopOf="@+id/guideline29" />


    <TextView
        android:id="@+id/num1"
        android:layout_width="50dp"
        android:layout_height="55dp"
        android:layout_marginStart="100dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/rectangle"
        android:fontFamily="@font/maplestorybold"
        android:gravity="center"
        android:text="1"
        android:textColor="#000"
        android:textSize="30dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/guideline29" />

    <EditText
        android:id="@+id/result1"
        android:layout_width="50dp"
        android:layout_height="55dp"
        android:layout_marginStart="10dp"
        android:layout_marginBottom="60dp"
        android:background="@drawable/rectangle"
        android:fontFamily="@font/maplestorybold"
        android:inputType="number"
        android:textColor="#000"
        android:textSize="35dp"
        app:layout_constraintBottom_toTopOf="@+id/guideline30"
        app:layout_constraintStart_toEndOf="@+id/sign2" />

    <EditText
        android:id="@+id/result2"
        android:layout_width="50dp
"
        android:layout_height="55dp"
        android:layout_marginStart="16dp"
        android:layout_marginBottom="60dp"
        android:background="@drawable/rectangle"
        android:fontFamily="@font/maplestorybold"
        android:inputType="number"
        android:textColor="#000"
        android:textSize="35dp"
        app:layout_constraintBottom_toTopOf="@+id/guideline30"
        app:layout_constraintStart_toEndOf="@+id/result1" />

    <TextView
        android:id="@+id/sign"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="20dp"
        android:text="+"
        android:textSize="50dp"
        app:layout_constraintStart_toEndOf="@+id/num1"
        app:layout_constraintTop_toTopOf="@+id/guideline29" />

    <TextView
        android:id="@+id/sign2"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:layout_marginStart="110dp"
        android:layout_marginTop="124dp"
        android:text="="
        android:textSize="50dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/guideline29" />

    <Button
        android:id="@+id/button6"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="20dp"
        android:fontFamily="@font/maplestorybold"
        android:text="6"
        android:textColor="#000"
        android:textSize="30sp"
        app:backgroundTint="@color/ic_logo_background"
        app:layout_constraintStart_toEndOf="@+id/button5"
        app:layout_constraintTop_toBottomOf="@+id/button3" />

    <Button
        android:id="@+id/button9"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="20dp"
        android:fontFamily="@font/maplestorybold"
        android:text="9"
        android:textColor="#000"
        android:textSize="30sp"
        app:backgroundTint="@color/ic_logo_background"
        app:layout_constraintStart_toEndOf="@+id/button8"
        app:layout_constraintTop_toBottomOf="@+id/button6" />

    <Button
        android:id="@+id/button3"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="20dp"
        android:fontFamily="@font/maplestorybold"
        android:text="3"
        android:textColor="#000"
        android:textSize="30sp"
        app:backgroundTint="@color/ic_logo_background"
        app:layout_constraintStart_toEndOf="@+id/button2"
        app:layout_constraintTop_toTopOf="@+id/guideline30" />

    <Button
        android:id="@+id/button1"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="95dp"
        android:fontFamily="@font/maplestorybold"
        android:text="1"
        android:textColor="#000"
        android:textSize="30sp"
        app:backgroundTint="@color/ic_logo_background"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/guideline30" />

    <Button
        android:id="@+id/button2"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="20dp"
        android:fontFamily="@font/maplestorybold"
        android:text="2"
        android:textColor="#000"
        android:textSize="30sp"
        app:backgroundTint="@color/ic_logo_background"
        app:layout_constraintStart_toEndOf="@+id/button1"
        app:layout_constraintTop_toTopOf="@+id/guideline30" />

    <Button
        android:id="@+id/button5"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="20dp"
        android:fontFamily="@font/maplestorybold"
        android:text="5"
        android:textColor="#000"
        android:textSize="30sp"
        app:backgroundTint="@color/ic_logo_background"
        app:layout_constraintStart_toEndOf="@+id/button4"
        app:layout_constraintTop_toBottomOf="@+id/button2" />

    <Button
        android:id="@+id/button8"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="20dp"
        android:fontFamily="@font/maplestorybold"
        android:text="8"
        android:textColor="#000"
        android:textSize="30sp"
        app:backgroundTint="@color/ic_logo_background"
        app:layout_constraintStart_toEndOf="@+id/button7"
        app:layout_constraintTop_toBottomOf="@+id/button5" />

    <Button
        android:id="@+id/button4"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="95dp"
        android:fontFamily="@font/maplestorybold"
        android:text="4"
        android:textColor="#000"
        android:textSize="30sp"
        app:backgroundTint="@color/ic_logo_background"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button1" />

    <Button
        android:id="@+id/button0"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:fontFamily="@font/maplestorybold"
        android:text="0"
        android:textColor="#000"
        android:textSize="30sp"
        app:backgroundTint="@color/ic_logo_background"
        app:layout_constraintBottom_toTopOf="@+id/guideline31"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button8"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/report"
        android:layout_width="100dp"
        android:layout_height="60dp"
        android:layout_marginStart="13dp"
        android:fontFamily="@font/maplestorybold"
        android:text="제출"
        android:textColor="#000"
        android:textSize="30sp"
        app:backgroundTint="@color/ic_logo_background"
        app:layout_constraintStart_toEndOf="@+id/button0"
        app:layout_constraintTop_toBottomOf="@+id/button9" />

    <Button
        android:id="@+id/button7"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="95dp"
        android:fontFamily="@font/maplestorybold"
        android:text="7"
        android:textColor="#000"
        android:textSize="30sp"
        app:backgroundTint="@color/ic_logo_background"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button4" />


</androidx.constraintlayout.widget.ConstraintLayout>